extends layout

block prepend title
  #{ page.title } -

block header
  include author
  a.back(href='/') << back to index
  h1= page.title
  p
    | written by:
    mixin author(page.metadata.author)

block content
 article.post
   header
     h2
       a(href=)= page.title
  
    section.content!= page.html
